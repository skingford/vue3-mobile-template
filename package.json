{
  "name": "vue3-moblie-template",
  "version": "0.0.1",
  "author": {
    "name": "kingford"
  },
  "scripts": {
    "dev": "vite --mode development",
    "build": "cross-env NODE_ENV=production vite build  && esno ./build/script/postBuild.ts",
    "build:test": "cross-env vite build --mode test && esno ./build/script/postBuild.ts",
    "build:no-cache": "yarn clean:cache && npm run build",
    "serve": "vite preview --port 8080",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "svgo": "svgo -f src/assets/icons/svg --config=src/icons/svgo.yml"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "dayjs": "^1.10.7",
    "fs-extra": "^10.0.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "vant": "^3.1.3",
    "vue": "^3.1.5",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/fs-extra": "^9.0.13",
    "@vitejs/plugin-legacy": "^1.5.3",
    "@vitejs/plugin-vue": "^1.9.0",
    "@vitejs/plugin-vue-jsx": "^1.1.8",
    "@vue/compiler-sfc": "^3.2.14",
    "@vue/eslint-config-prettier": "^6.0.0",
    "autoprefixer": "^10.3.5",
    "chalk": "^4.1.2",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.18.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-webpack-plugin": "^3.0.1",
    "typescript": "^4.4.3",
    "vite": "^2.5.10",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-html": "^2.1.0",
    "vite-plugin-imagemin": "^0.4.5",
    "vite-plugin-pwa": "^0.11.2",
    "vite-plugin-style-import": "^1.2.1",
    "vite-plugin-svg-icons": "^1.0.4",
    "vite-plugin-windicss": "^1.4.4",
    "vue-tsc": "^0.3.0"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,vue}": [
      "prettier --tab-width 2 --write",
      "vite lint --fix",
      "git add"
    ]
  }
}
